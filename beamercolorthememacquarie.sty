\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthememacquarie}[2017/01/23 Metropolis color theme]
\RequirePackage{pgfopts}
\pgfkeys{
  /macquarie/color/block/.cd,
    .is choice,
    transparent/.code=\macquarie@block@transparent,
    fill/.code=\macquarie@block@fill,
}
\pgfkeys{
  /macquarie/color/background/.cd,
    .is choice,
    dark/.code=\macquarie@colors@dark,
    light/.code=\macquarie@colors@light,
}
\newcommand{\macquarie@color@setdefaults}{
  \pgfkeys{/macquarie/color/.cd,
    background=light,
    block=transparent,
  }
}
\definecolor{mqRed}{HTML}{A6192E}
\definecolor{mqDeepRed}{HTML}{76232F}
\definecolor{mqBrightRed}{HTML}{D6001C}
\definecolor{mqMagenta}{HTML}{C6007E}
\definecolor{mqPurple}{HTML}{80225F}
\definecolor{mqCharcoal}{HTML}{373A36}
\definecolor{mqSand}{HTML}{D6D2C4}
\definecolor{mqBlack}{HTML}{000000}
\definecolor{mqWhite}{HTML}{FFFFFF}

\newcommand{\macquarie@colors@dark}{
  \setbeamercolor{normal text}{%
    fg=mqWhite,
    bg=mqBlack
  }
  \usebeamercolor[fg]{normal text}
}
\newcommand{\macquarie@colors@light}{
  \setbeamercolor{normal text}{%
    fg=mqBlack,
    bg=mqWhite
  }
}

\setbeamercolor{header text}{%
  fg=mqRed,
  bg=mqSand
}
\setbeamercolor{alerted text}{%
  fg=mqBrightRed
}
\setbeamercolor{example text}{%
  fg=mqBrightRed
}
\setbeamercolor{titlelike}{fg=mqBlack, bg=mqSand}
\setbeamercolor{author}{use=header text, parent=header text}
\setbeamercolor{date}{use=header text, parent=header text}
\setbeamercolor{institute}{use=header text, parent=header text}
\setbeamercolor{structure}{use=header text, fg=header text.fg}
\setbeamercolor{palette primary}{%
  use=normal text,
  fg=mqRed,
  bg=mqSand
}
\setbeamercolor{frametitle}{%
  use=palette primary,
  parent=palette primary
}
\setbeamercolor{progress bar}{%
  use=alerted text,
  fg=alerted text.fg,
  bg=alerted text.fg!50!black!30
}
\setbeamercolor{title separator}{
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in head/foot}{%
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in section page}{
  use=progress bar,
  parent=progress bar
}
\newcommand{\macquarie@block@transparent}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=
  }
  \setbeamercolor{block body}{
    bg=
  }
}
\newcommand{\macquarie@block@fill}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=normal text.bg!80!fg
  }
  \setbeamercolor{block body}{
    use={block title, normal text},
    bg=block title.bg!50!normal text.bg
  }
}
\setbeamercolor{block title alerted}{%
    use={block title, alerted text},
    bg=block title.bg,
    fg=alerted text.fg
}
\setbeamercolor{block title example}{%
    use={block title, example text},
    bg=block title.bg,
    fg=example text.fg
}
\setbeamercolor{block body alerted}{use=block body, parent=block body}
\setbeamercolor{block body example}{use=block body, parent=block body}
\setbeamercolor{footnote}{fg=normal text.fg!90}
\setbeamercolor{footnote mark}{fg=.}
\setbeamercolor{bibliography entry author}{fg=, bg=}
\setbeamercolor{bibliography entry title}{fg=, bg=}
\setbeamercolor{bibliography entry location}{fg=, bg=}
\setbeamercolor{bibliography entry note}{fg=, bg=}
\macquarie@color@setdefaults
\ProcessPgfPackageOptions{/macquarie/color}
\mode<all>
\endinput
